
<DOMAIN> C[0-1] </DOMAIN>
</GEOMETRY>
<EXPANSIONS>
    <E COMPOSITE="C[0]" NUMMODES="3" TYPE="MODIFIED" FIELDS="rho,rhou,rhov,rhow, E" />
    <E COMPOSITE="C[1]" NUMMODES="3" TYPE="MODIFIED" FIELDS="rho,rhou,rhov,rhow, E" />
</EXPANSIONS>
<CONDITIONS>

        <!-- Airbus Test Case -->
        <!-- M = M-straight * cos(Lambda),-->
        <PARAMETERS>
            <P> TimeStep            = 0.00000000002                     </P>
            <P> NumSteps            = 3                                 </P>
            <P> FinTime             = NumSteps*TimeStep                 </P>
            <P> IO_CheckSteps       = 100000                            </P>
            <P> IO_InfoSteps        = 1                                 </P>
            <P> Gamma               = 1.4                               </P>
            <P> GasConstant         = 287.058                           </P>
            <P> pInf                = 22600                             </P>
            <P> rhoInf              = 0.364                             </P>
            <P> TInf                = pInf/(287.058*rhoInf)             </P>
            <P> Mach                = 0.5                               </P>
            <P> cInf                = sqrt(Gamma*287.058*TInf)          </P>
            <P> alpha               = 0.0                               </P>
            <P> uInf                = Mach*cInf*cos(alpha*PI/180)       </P>
            <P> vInf                = Mach*cInf*sin(alpha*PI/180)       </P>
            <P> wInf                = 0.0                               </P>
            <P> Twall               = 258                               </P>
            <P> Re                  = 40000                             </P>
            <P> L                   = 0.1                               </P>
            <P> mu                  = rhoInf * L * uInf / Re            </P>
            <P> Pr                  = 0.72                              </P>
            <P> Cp                  = (Gamma / (Gamma - 1))*GasConstant </P>
        </PARAMETERS>
        <SOLVERINFO>
            <I PROPERTY="EQType"                VALUE="NavierStokesCFE"     />
            <I PROPERTY="Projection"            VALUE="DisContinuous"       />
            <I PROPERTY="AdvectionType"         VALUE="WeakDG"              />
            <I PROPERTY="DiffusionType"         VALUE="LDGNS"               />
            <I PROPERTY="TimeIntegrationMethod" VALUE="RungeKutta2_ImprovedEuler"/>
            <I PROPERTY="UpwindType"            VALUE="ExactToro"           />
            <I PROPERTY="ProblemType"           VALUE="General"             />
            <I PROPERTY="ViscosityType"         VALUE="Variable"            />
        </SOLVERINFO>
        <VARIABLES>
            <V ID="0"> rho  </V>
            <V ID="1"> rhou </V>
            <V ID="2"> rhov </V>
            <V ID="3"> rhow </V>
            <V ID="4"> E    </V>
        </VARIABLES>
        <BOUNDARYREGIONS>
            <B ID="0">  C[2]  </B>
            <B ID="1">  C[3]  </B>
            <B ID="2">  C[4]  </B>
            <B ID="3">  C[5]  </B>
        </BOUNDARYREGIONS>
       <BOUNDARYCONDITIONS>
        <REGION REF="0">
                <D VAR="rho"  USERDEFINEDTYPE="WallViscous" VALUE="0" />
                <D VAR="rhou" USERDEFINEDTYPE="WallViscous" VALUE="0" />
                <D VAR="rhov" USERDEFINEDTYPE="WallViscous" VALUE="0" />
                <D VAR="rhow" USERDEFINEDTYPE="WallViscous" VALUE="0" />
                <D VAR="E"    USERDEFINEDTYPE="WallViscous" VALUE="0" />
            </REGION>
         <REGION REF="1">
                <D VAR="rho"  USERDEFINEDTYPE="rhoInf" VALUE="0" />
                <D VAR="rhou" USERDEFINEDTYPE="rhoInf*uInf" VALUE="0" />
                <D VAR="rhov" USERDEFINEDTYPE="rhoInf*vInf" VALUE="0" />
                <D VAR="rhow" USERDEFINEDTYPE="rhoInf*wInf" VALUE="0" />
                <D VAR="E"    USERDEFINEDTYPE="pInf / (Gamma - 1) + 0.5 * rhoInf * (uInf * uInf + vInf * vInf)" VALUE="0" />
            </REGION>
         <REGION REF="2">
             <D VAR="rho"  USERDEFINEDTYPE="rhoInf" VALUE="0" />
             <D VAR="rhou" USERDEFINEDTYPE="rhoInf*uInf" VALUE="0" />
             <D VAR="rhov" USERDEFINEDTYPE="rhoInf*vInf" VALUE="0" />
             <D VAR="rhow" USERDEFINEDTYPE="rhoInf*wInf" VALUE="0" />
             <D VAR="E"    USERDEFINEDTYPE="pInf / (Gamma - 1) + 0.5 * rhoInf * (uInf * uInf + vInf * vInf)" VALUE="0" />
            </REGION>

        <REGION REF="3">
                <D VAR="rho"  USERDEFINEDTYPE="PressureOutflow" VALUE="0" />
                <D VAR="rhou" USERDEFINEDTYPE="PressureOutflow" VALUE="0" />
                <D VAR="rhov" USERDEFINEDTYPE="PressureOutflow" VALUE="0" />
                <D VAR="rhow" USERDEFINEDTYPE="PressureOutflow" VALUE="0" />
                <D VAR="E"    USERDEFINEDTYPE="PressureOutflow" VALUE="0" />
            </REGION>
    </BOUNDARYCONDITIONS>
    <FUNCTION NAME="InitialConditions">
        <E VAR="rho"  USERDEFINEDTYPE="rhoInf" VALUE="0" />
        <E VAR="rhou" USERDEFINEDTYPE="rhoInf*uInf" VALUE="0" />
        <E VAR="rhov" USERDEFINEDTYPE="rhoInf*vInf" VALUE="0" />
        <E VAR="rhow" USERDEFINEDTYPE="rhoInf*wInf" VALUE="0" />
        <E VAR="E"    USERDEFINEDTYPE="pInf / (Gamma - 1) + 0.5 * rhoInf * (uInf * uInf + vInf * vInf)" VALUE="0" />
    </FUNCTION>
</CONDITIONS>
